{
	"name": "meta4mvp",
	"version": "0.0.1",
	"home": "src",
	"basePath": "/",
	"host": "localhost",
	"port": 8090,
	"salt": "-change-me-to-anything-",
	"features": {
		"meta4ux": {},
		"assets": {
			"disabled": false,
			"path": "/",
			"home": "src/public"
		},
		"upload": {
			"home": "tmp/uploads",
			"baseURL": "/meta4mvp/upload/",
			"limits": {
				"fieldNameSize": 100,
				"files": 2,
				"fields": 5
			}
		}
	},
	"skeleton": {
		"models": {
			"user": {
				"adapter": "neodb",
				"widget": "Form"
			},
			"todo": {
				"adapter": "neodb",
				"widget": "CRUD"
			},
			"help": {
				"adapter": "neodb",
				"widget": "CRUD"
			}
		}
	}
}